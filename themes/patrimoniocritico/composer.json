{
  "name": "10uppatrimoniocritico/theme",
  "type": "wordpress-theme",
  "authors": [
    {
      "name": "patrimoniocritico",
      "email": "info@patrimoniocritico.cl"
    }
  ],
  "require": {
    "php": ">=8.0",
    "haydenpierce/class-finder": "^0.4.3"
  },
  "require-dev": {
    "10up/phpcs-composer": "dev-trunk"
  },
  "scripts": {
    "lint": "phpcs .",
    "lint-fix": "phpcbf ."
  },
  "autoload": {
    "psr-4": {
      "PatrimonioCritico\\": "includes/classes/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
